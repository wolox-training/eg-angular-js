{
  "name": "wbooks",
  "version": "1.0.0",
  "description": "Wolox Books Training App",
  "main": "index.js",
  "scripts": {
    "postinstall": "npm run copy-libs",
    "update-deps": "npm update",
    "postupdate-deps": "npm run copy-libs",
    "copy-libs": "cpx \"node_modules/{angular,angular-*,@uirouter/angularjs}/**/*\" app/lib -C",
    "prestart": "npm install",
    "start": "concurrently --kill-others \"npx babel src -d app --watch\" \"http-server -a localhost -p 8000 -c-1 ./app\"",
    "pretest": "npm install",
    "build": "babel src -d app"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wolox-training/eg-angular-js.git"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@uirouter/angularjs": "^1.0.20",
    "angular": "^1.7.5",
    "angular-loader": "^1.7.5",
    "angular-resource": "^1.7.5",
    "angular-route": "^1.7.5",
    "pug": "^2.0.3",
    "wolox-equalizer": "0.0.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.2.3",
    "angular-mocks": "^1.7.5",
    "concurrently": "^4.1.0",
    "cpx": "^1.5.0",
    "gulp": "^4.0.0",
    "gulp-pug": "^4.0.1",
    "gulp-sass": "^4.0.2",
    "http-server": "^0.11.1",
    "jasmine-core": "^3.3.0",
    "karma": "^3.1.1",
    "karma-chrome-launcher": "^2.2.0",
    "karma-firefox-launcher": "^1.1.0",
    "karma-jasmine": "^1.1.2",
    "node-sass": "^4.11.0",
    "protractor": "^5.4.1"
  },
  "bugs": {
    "url": "https://github.com/wolox-training/eg-angular-js/issues"
  },
  "homepage": "https://github.com/wolox-training/eg-angular-js#readme"
}
